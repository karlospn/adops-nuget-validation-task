{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\buildAndReleaseTask\\index.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "INPUT_usePrereleaseNugets": "false",
                "SYSTEM_DEFAULTWORKINGDIRECTORY": "C:\\MyTestingApp",
                "Build_SourceBranch": "/ref/heads/master",
                "ENDPOINT_AUTH_SYSTEMVSSCONNECTION": "{\"parameters\":{\"AccessToken\":\"SOME PAT GOES HERE\"},\"scheme\":\"OAuth\"}"
            }
        }
    ]
}